{
    "plugins": ["@typescript-eslint", "react", "react-hooks"],
    "parser": "@typescript-eslint/parser",
    //   "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "worker": true,
        "commonjs": true,
        "amd": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    //   "plugins": ["react", "react-hooks"],
    "rules": {
        "react/prop-types": 0
    }
}
